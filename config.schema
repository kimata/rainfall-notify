{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "liveness": {
            "type": "object",
            "properties": {
                "file": {
                    "type": "object",
                    "properties": {
                        "watch": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "watch"
                    ]
                }
            },
            "required": [
                "file"
            ]
        },
        "influxdb": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "org": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                },
                "bucket": {
                    "type": "string"
                }
            },
            "required": [
                "bucket",
                "org",
                "token",
                "url"
            ]
        },
        "voice": {
            "type": "object",
            "properties": {
                "server": {
                    "type": "object",
                    "properties": {
                        "url": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "url"
                    ]
                }
            },
            "required": [
                "server"
            ]
        },
        "rain_cloud": {
            "type": "object",
            "properties": {
                "view": {
                    "type": "object",
                    "properties": {
                        "url": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "url"
                    ]
                },
                "img": {
                    "type": "object",
                    "properties": {
                        "url_tmpl": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "url_tmpl"
                    ]
                }
            },
            "required": [
                "img",
                "view"
            ]
        },
        "rain_fall": {
            "type": "object",
            "properties": {
                "sensor": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "type": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "name",
                        "type"
                    ]
                },
                "forecast": {
                    "type": "object",
                    "properties": {
                        "tenki": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "url"
                            ]
                        }
                    },
                    "required": [
                        "tenki"
                    ]
                }
            },
            "required": [
                "forecast",
                "sensor"
            ]
        },
        "weather": {
            "type": "object",
            "properties": {
                "forecast": {
                    "type": "object",
                    "properties": {
                        "yahoo": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "url"
                            ]
                        }
                    },
                    "required": [
                        "yahoo"
                    ]
                }
            },
            "required": [
                "forecast"
            ]
        },
        "notify": {
            "type": "object",
            "properties": {
                "line": {
                    "type": "object",
                    "properties": {
                        "channel": {
                            "type": "object",
                            "properties": {
                                "access_token": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "access_token"
                            ]
                        }
                    },
                    "required": [
                        "channel"
                    ]
                },
                "voice": {
                    "type": "object",
                    "properties": {
                        "hour": {
                            "type": "object",
                            "properties": {
                                "start": {
                                    "type": "integer"
                                },
                                "end": {
                                    "type": "integer"
                                }
                            },
                            "required": [
                                "end",
                                "start"
                            ]
                        },
                        "chime": {
                            "type": "object",
                            "properties": {
                                "file": {
                                    "type": "string"
                                },
                                "duration": {
                                    "type": "integer"
                                }
                            },
                            "required": [
                                "duration",
                                "file"
                            ]
                        }
                    },
                    "required": [
                        "hour"
                    ]
                },
                "footprint": {
                    "type": "object",
                    "properties": {
                        "voice": {
                            "type": "object",
                            "properties": {
                                "file": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "file"
                            ]
                        },
                        "line": {
                            "type": "object",
                            "properties": {
                                "file": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "file"
                            ]
                        }
                    },
                    "required": [
                        "line",
                        "voice"
                    ]
                }
            },
            "required": [
                "footprint",
                "line",
                "voice"
            ]
        },
        "watch": {
            "type": "object",
            "properties": {
                "interval_sec": {
                    "type": "integer"
                }
            },
            "required": [
                "interval_sec"
            ]
        }
    },
    "required": [
        "influxdb",
        "liveness",
        "notify",
        "rain_cloud",
        "rain_fall",
        "voice",
        "watch",
        "weather"
    ]
}
